{
  "name": "ui-shell",
  "description": "Stream shell app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:run\" \"npm run build:css\"",
    "dev:run": "next dev --turbopack -p 3000",
    "build:css": "npx @tailwindcss/cli -i ./app/tailwind.css -o ./public/css/tailwind.css --watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@first-apps/shared": "^1.1.15",
    "@tailwindcss/cli": "^4.0.9",
    "next": "15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "4",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.1.2",
    "typescript": "^5"
  }
}
